# Backend Selection
# Set to 'true' to use AWS Lambda backend, or 'false' (or omit) to use Supabase
REACT_APP_USE_AWS=false

# AWS Configuration (only needed if REACT_APP_USE_AWS=true)
# Set this to your API Gateway endpoint after deploying the AWS stack
# Example: https://abcd1234.execute-api.us-east-1.amazonaws.com/dev
REACT_APP_AWS_API_URL=

# AWS Region where resources are deployed
REACT_APP_AWS_REGION=us-east-1

# AWS Stack name
REACT_APP_AWS_STACK_NAME=stock-analysis-app
